{
	"name": "@sassysaint/client",
	"version": "3.3.0",
	"license": "MIT",
	"author": "Arno Versini",
	"type": "module",
	"private": true,
	"scripts": {
		"clean": "rimraf dist build",
		"dev": "rsbuild dev",
		"build": "tsc && rsbuild build",
		"build:bundlesize": "rsbuild build -c \"../../configuration/rsbuild-bundlesize.config.js\" ",
		"lint": "biome lint src",
		"start": "static-server dist --port 5173",
		"stats:pr": "bundlesize -c bundlesize.config.js -p \"$npm_package_version\" -o tmp/stats.json --silent",
		"stats:release": "bundlesize -c bundlesize.config.js -p \"$npm_package_version\" -o stats/stats.json --silent",
		"stats:report": "bundlesize -c bundlesize.config.js --type report -o tmp/pr-stats.md --silent",
		"stats": "bundlesize -c bundlesize.config.js -p \"$npm_package_version\"",
		"test": "vitest run",
		"test:coverage": "vitest run --coverage",
		"test:coverage:ui": "vitest --coverage --ui",
		"test:watch": "vitest"
	},
	"devDependencies": {
		"@versini/ui-styles": "1.9.3"
	},
	"dependencies": {
		"@versini/auth-provider": "6.0.0",
		"@versini/ui-components": "5.20.0",
		"@versini/ui-form": "1.3.6",
		"@versini/ui-hooks": "4.0.1",
		"@versini/ui-icons": "1.11.0",
		"@versini/ui-system": "1.4.2",
		"autoprefixer": "10.4.19",
		"postcss": "8.4.39",
		"pretty-ms": "9.0.0",
		"react-dom": "18.3.1",
		"react-markdown": "9.0.1",
		"react-use": "17.5.0",
		"react": "18.3.1",
		"rehype-highlight": "7.0.0",
		"rehype-katex": "7.0.0",
		"remark-gfm": "4.0.0",
		"remark-math": "6.0.0",
		"uuid": "10.0.0"
	}
}
