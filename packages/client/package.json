{
	"name": "@sassysaint/client",
	"version": "5.8.2",
	"license": "MIT",
	"author": "Arno Versini",
	"type": "module",
	"private": true,
	"scripts": {
		"clean": "rimraf dist build",
		"dev": "rsbuild dev",
		"build": "npm-run-all clean build:js",
		"build:js": "rsbuild build",
		"build:bundlesize": "rsbuild build -c \"../../configuration/rsbuild-bundlesize.config.js\" ",
		"lint": "biome lint src",
		"start": "static-server dist --port 5173",
		"stats:pr": "bundlesize -c bundlesize.config.js -p \"$npm_package_version\" -o tmp/stats.json --silent",
		"stats:release": "bundlesize -c bundlesize.config.js -p \"$npm_package_version\" -o stats/stats.json --silent",
		"stats:report": "bundlesize -c bundlesize.config.js --type report -o tmp/pr-stats.md --silent",
		"stats": "bundlesize -c bundlesize.config.js -p \"$npm_package_version\"",
		"test": "vitest run",
		"test:coverage": "vitest run --coverage",
		"test:coverage:ui": "vitest --coverage --ui",
		"test:watch": "vitest"
	},
	"devDependencies": {
		"@versini/ui-styles": "1.14.0"
	},
	"dependencies": {
		"@versini/auth-provider": "7.4.0",
		"@versini/ui-bubble": "1.4.0",
		"@versini/ui-button": "1.6.0",
		"@versini/ui-card": "1.4.0",
		"@versini/ui-footer": "1.4.0",
		"@versini/ui-hooks": "4.3.0",
		"@versini/ui-icons": "1.19.0",
		"@versini/ui-main": "1.4.0",
		"@versini/ui-menu": "1.4.0",
		"@versini/ui-panel": "1.2.2",
		"@versini/ui-spinner": "1.4.0",
		"@versini/ui-system": "1.8.0",
		"@versini/ui-table": "1.5.1",
		"@versini/ui-textarea": "1.4.0",
		"@versini/ui-textinput": "1.6.0",
		"@versini/ui-toggle": "1.4.0",
		"@versini/ui-togglegroup": "1.5.0",
		"@versini/ui-truncate": "1.1.7",
		"autoprefixer": "10.4.20",
		"postcss": "8.4.49",
		"pretty-ms": "9.2.0",
		"react-dom": "19.0.0",
		"react-markdown": "9.0.1",
		"react-use": "17.6.0",
		"react": "19.0.0",
		"rehype-highlight": "7.0.1",
		"rehype-katex": "7.0.1",
		"remark-gfm": "4.0.0",
		"remark-math": "6.0.0",
		"uuid": "11.0.3"
	}
}
